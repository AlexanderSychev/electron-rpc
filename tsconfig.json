{
    "compilerOptions": {
        "target": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "module": "esnext",
        "lib": ["esnext", "dom"],
    }
}
